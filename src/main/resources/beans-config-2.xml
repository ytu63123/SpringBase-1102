<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
<!-- 
    autowrie:
    
    
    -->
    <bean id="oracleProgrammer" class="com.mycompany.springbasic1102.di.byname.OracleProgrammer" autowire="byName">
        <property name="name" value="John" />
    </bean>
    
      <bean id="microsoftProgrammer" class="com.mycompany.springbasic1102.di.byname.MicrosoftProgrammer" autowire="byName">
        <property name="name" value="Mary" />
    </bean>
    

    <bean id="java" class="com.mycompany.springbasic1102.di.byname.Language">
        <property name="name" value="JAVA" />    
    </bean>
    
    <bean id="vb" class="com.mycompany.springbasic1102.di.byname.Language">
        <property name="name" value="VB" />    
    </bean>
    
</beans>
